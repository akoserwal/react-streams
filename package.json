{
  "name": "react-streams",
  "version": "9.6.0",
  "description": "Simple Streams for React",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnlindquist/react-streams.git"
  },
  "bugs": {
    "url": "https://github.com/johnlindquist/react-streams/issues"
  },
  "homepage": "https://github.com/johnlindquist/react-streams#readme",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "examples:client": "poi --config examples/poi.config.js --port 4321",
    "examples:server": "json-server --port 4322 examples/db.js",
    "cypress:run": "cypress run --config videoRecording=false",
    "cypress:open": "cypress open",
    "e2e": "run-p examples:* cypress:run",
    "e2e:open": "run-p examples:* cypress:open",
    "docs": "poi --config docs/poi.config.js"
  },
  "keywords": [
    "react",
    "rxjs",
    "streams"
  ],
  "author": "John Lindquist",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-0": "^7.0.0-beta.46",
    "@cypress/webpack-preprocessor": "^2.0.1",
    "@mdx-js/loader": "^0.8.1",
    "@mdx-js/mdx": "^0.8.1",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.2",
    "babel-preset-stage-0": "^6.24.1",
    "cypress": "^2.1.0",
    "jest": "^22.4.3",
    "json-server": "^0.12.2",
    "poi": "^10.1.5",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-loadable": "^5.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "rxjs": "^6.1.0",
    "ts-jest": "^22.4.4",
    "ts-snippet": "^3.1.1",
    "typescript": "^2.8.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig-test.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "npm-run-all": "^4.1.2"
  }
}